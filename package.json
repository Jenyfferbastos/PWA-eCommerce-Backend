{
  "name": "api-e-commerce",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 rm -rf ./dist/tst && tsc -p . && rm -rf ./dist/tst",
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 jest --coverage --runInBand --detectOpenHandles --forceExit --watchAll",
    "test:watch": "jest --runInBand --detectOpenHandles --watchAll",
    "make-badges": "istanbul-badges-readme",
    "sonar": "sonar-scanner -X",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix",
    "db:create": "ts-node utils/createDB.ts",
    "db:drop": "ts-node utils/dropDB.ts",
    "db:migrate:run": "ts-node ./node_modules/.bin/typeorm migration:run -d src/infra/database/postgres/ormconfig.ts",
    "db:migrate:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "db:migrate:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -d src/infra/database/postgres/ormconfig.ts",
    "db:migrate:revert": "ts-node ./node_modules/.bin/typeorm migration:revert -d src/infra/database/postgres/ormconfig.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "moment": "2.29.1"
  },
