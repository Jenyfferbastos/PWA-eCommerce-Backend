    "class-validator-jsonschema": "^3.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-http-context": "^1.2.4",
    "http-status": "^1.5.0",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-name": "0.0.1-security",
    "mongoose": "^5.13.7 ",
    "query-string": "^7.1.0",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.7.0",
    "tsc-node": "0.0.3",
    "typeorm": "^0.3.7",
