    "@types/body-parser": "^1.19.2",
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/elasticsearch": "^5.0.40",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
